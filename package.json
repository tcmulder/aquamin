{
	"name": "aquamin",
	"version": "4.0.0",
	"description": "Parcel build tool for Aquamarine",
	"private": true,
	"author": "@tcmulder",
	"license": "ISC",
	"engines": {
		"node": ">=20.12.1",
		"npm": ">=10.5.0"
	},
	"scripts": {
		"docs": "docsify serve docs --open --port 4000",
		"lint": "eslint --config=./assets/config/.eslintrc.js ./assets",
		"lint:fix": "eslint --config=./assets/config/.eslintrc.js ./assets --fix",
		"start": "wp-scripts start --config=includes/config/webpack.config.js --webpack-src-dir=assets --output-path=dist --webpack-copy-php",
		"build": "wp-scripts build --config=includes/config/webpack.config.js --webpack-src-dir=assets --output-path=dist --webpack-copy-php"
	},
	"browserslist": "> 0.25%, last 2 versions, not dead",
	"eslintConfig": {
		"extends": "./assets/config/.eslintrc.js"
	},
	"devDependencies": {
		"@svgr/webpack": "^8.1.0",
		"@wordpress/scripts": "^27.6.0",
		"browser-sync-webpack-plugin": "^2.3.0",
		"classnames": "^2.5.1",
		"copy-webpack-plugin": "^12.0.2",
		"docsify-cli": "^4.4.4",
		"dotenv": "^16.4.5",
		"filename-replace-webpack-plugin": "^1.0.1",
		"glob": "^10.3.12",
		"path": "^0.12.7",
		"postcss-loader": "^8.1.1",
		"postcss-preset-env": "^9.5.4",
		"webpack-import-glob-loader": "^1.6.3",
		"webpack-remove-empty-scripts": "^1.0.4"
	},
	"dependencies": {
		"@wordpress/icons": "^9.46.0",
		"@wordpress/interactivity": "^5.4.0"
	}
}
