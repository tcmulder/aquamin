  <content:encoded><![CDATA[<!-- wp:html -->
<div class="size-check has-global-padding is-layout-constrained">
	<style>
		:root {
			--sc-c-reference-normal: lime;
			--sc-c-reference-wide: yellow;
			--sc-c-reference-full: aqua;
		}
		.torso .wp-block-group,
		.torso .wp-block-group__inner-container,
		.torso .wp-block-columns,
		.torso .wp-block-column {
			outline: 1px solid rgba(0, 0, 0, 0.2);
		}
		.torso .has-background {
			outline: 1px solid rgba(0, 0, 0, 0.2);
		}
		.torso .has-background .has-background {
			outline: 1px solid rgba(0, 0, 0, 0.2);
		}
		.size-check {
			pointer-events: none;
			position: fixed;
			inset: auto 0 0;
			z-index: 1000;
			width: 100% !important;
			max-width: 100% !important;
		}
		.size-check div {
			background: currentcolor;
			margin: auto;
			position: relative;
		}
		.size-check span {
			float: right;
		}
		.size-check i {
			pointer-events: none;
			border: 1px dashed currentColor;
			bottom: 0;
			height: 9999px;
			position: absolute;
			width: var(--gutter);
		}
		.size-check i:first-of-type {
			right: 100%;
		}
		.size-check i:last-of-type {
			left: 100%;
		}
		.size-check i {
			opacity: 0.5;
		}
		.size-check b {
			color: black !important;
		}
		.size-check__container {
			padding: 0 var(--gutter);
		}
		.size-check__container--normal,
		.size-check__container--normal div {
			color: var(--sc-c-reference-normal);
		}
		.size-check__container--wide,
		.size-check__container--wide div {
			color: var(--sc-c-reference-wide);
		}
		.size-check__container--full,
		.size-check__container--full div {
			color: var(--sc-c-reference-full);
		}
	</style>
	<div class="size-check__container size-check__container--normal">
		<div>
			<i></i><i></i>
			<b>[ Normal <span>]</span></b>
		</div>
		<i></i><i></i>
	</div>
	<div class="size-check__container size-check__container--wide alignwide">
		<div>
			<i></i><i></i>
			<b>[ Wide <span>]</span></b>
		</div>
		<i></i><i></i>
	</div>
	<div class="size-check__container size-check__container--full alignfull">
		<div>
			<i></i><i></i>
			<b>[ Full <span>]</span></b>
		</div>
		<i></i><i></i>
	</div>
	<script>
		const sizer = () => {
			document.querySelectorAll('.size-check__container ')
				.forEach((el) => {
					el.querySelector('span').innerHTML = `${el.offsetWidth}px ]`;
				});
			document.querySelectorAll('h4 strong')
				.forEach(el=>{
					el.innerHTML = `${el.parentElement.offsetWidth}px`;
				});
		}
		window.addEventListener('load', sizer);
		window.addEventListener('resize', sizer);
		window.addEventListener('load', () => {
			document.querySelectorAll('h3 strong')
				.forEach(el=>{
					if (el.innerText === 'column width') {
						el.innerText += `(${getComputedStyle(document.body).getPropertyValue('--wp--style--global--content-size').trim()})`;
					} else if (el.innerText === 'wide width') {
						el.innerText += `(${getComputedStyle(document.body).getPropertyValue('--wp--style--global--wide-size').trim()})`;
					} else if (el.innerText === 'full width') {
						el.innerText += `(${window.innerWidth}px)`;
					}
				});
		})
	</script>
</div>
