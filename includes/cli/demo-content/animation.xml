<!-- /wp:heading -->

<!-- wp:paragraph {"align":"center","placeholder":"Write title…","textColor":"0-000","aquaminClassNameAni":[{"value":"ani\u002d\u002dscale","title":"Scale"}]} -->
<p class="has-text-align-center has-0-000-color has-text-color ani--scale ani">scale</p>
<!-- /wp:paragraph --></div></div>
<!-- /wp:cover -->

<!-- wp:cover {"dimRatio":10,"overlayColor":"0-000","minHeight":50,"minHeightUnit":"vh","align":"full"} -->
