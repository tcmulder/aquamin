<h2 class="has-text-align-center">Background:</h2>
<!-- /wp:heading -->

<!-- wp:paragraph {"align":"center"} -->
<p class="has-text-align-center">This block's background will fade in behind itself and sibling blocks.<br></p>
<!-- /wp:paragraph -->

<!-- wp:paragraph {"align":"center"} -->
<p class="has-text-align-center">(If you don't need this, you can disable it in <code>blocks/block-library/extended-ani/script.js</code>)</p>
<!-- /wp:paragraph --></div></div>
<!-- /wp:cover -->

<!-- wp:cover {"dimRatio":0,"overlayColor":"0-900","minHeight":50,"minHeightUnit":"vh","isDark":false,"align":"full"} -->
