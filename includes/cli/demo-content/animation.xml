<!-- /wp:cover -->

<!-- wp:cover {"dimRatio":10,"overlayColor":"0-000","minHeight":50,"minHeightUnit":"vh","align":"full"} -->
<div class="wp-block-cover alignfull" style="min-height:50vh"><span aria-hidden="true" class="wp-block-cover__background has-0-000-background-color has-background-dim-10 has-background-dim"></span><div class="wp-block-cover__inner-container"><!-- wp:heading {"textAlign":"center","textColor":"0-000"} -->
<h2 class="has-text-align-center has-0-000-color has-text-color">Left:</h2>
<!-- /wp:heading -->

<!-- wp:paragraph {"align":"center","placeholder":"Write title…","textColor":"0-000","aquaminClassNameAni":[{"value":"ani\u002d\u002dleft","title":"Left"}]} -->
<p class="has-text-align-center has-0-000-color has-text-color ani--left ani">left</p>
<!-- /wp:paragraph --></div></div>
<!-- /wp:cover -->

<!-- wp:cover {"dimRatio":10,"overlayColor":"0-000","minHeight":50,"minHeightUnit":"vh","align":"full"} -->
<div class="wp-block-cover alignfull" style="min-height:50vh"><span aria-hidden="true" class="wp-block-cover__background has-0-000-background-color has-background-dim-10 has-background-dim"></span><div class="wp-block-cover__inner-container"><!-- wp:heading {"textAlign":"center","textColor":"0-000"} -->
<h2 class="has-text-align-center has-0-000-color has-text-color">Right:</h2>
<!-- /wp:heading -->

<!-- wp:paragraph {"align":"center","placeholder":"Write title…","textColor":"0-000","aquaminClassNameAni":[{"value":"ani\u002d\u002dright","title":"Right"}]} -->
<p class="has-text-align-center has-0-000-color has-text-color ani--right ani">right</p>
<!-- /wp:paragraph --></div></div>
<!-- /wp:cover -->

<!-- wp:cover {"dimRatio":10,"overlayColor":"0-000","minHeight":50,"minHeightUnit":"vh","align":"full"} -->
<div class="wp-block-cover alignfull" style="min-height:50vh"><span aria-hidden="true" class="wp-block-cover__background has-0-000-background-color has-background-dim-10 has-background-dim"></span><div class="wp-block-cover__inner-container"><!-- wp:heading {"textAlign":"center","textColor":"0-000"} -->
