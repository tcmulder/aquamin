  <content:encoded><![CDATA[<!-- wp:paragraph -->
<p class="">Here are common animations. Apply to your own blocks via <code>blocks/block-library/extended-ani/index.js@affectedBlocks</code>.</p>
<!-- /wp:paragraph -->

<!-- wp:cover {"dimRatio":10,"overlayColor":"0-000","minHeight":50,"minHeightUnit":"vh","align":"full"} -->
<div class="wp-block-cover alignfull" style="min-height:50vh"><span aria-hidden="true" class="wp-block-cover__background has-0-000-background-color has-background-dim-10 has-background-dim"></span><div class="wp-block-cover__inner-container"><!-- wp:paragraph {"align":"center","placeholder":"Write title…","textColor":"0-000","fontSize":"large","aquaminClassNameAni":[]} -->
<p class="has-text-align-center has-0-000-color has-text-color has-large-font-size">On Load:</p>
<!-- /wp:paragraph -->

<!-- wp:paragraph {"align":"center","textColor":"0-000","className":"ani\u002d\u002dload","aquaminClassNameAni":[{"value":"ani\u002d\u002dopacity","title":"Opacity"}]} -->
<p class="has-text-align-center ani--load has-0-000-color has-text-color ani--opacity ani">On Load</p>
<!-- /wp:paragraph -->

<!-- wp:paragraph {"align":"center","textColor":"0-000"} -->
<p class="has-text-align-center has-0-000-color has-text-color"><br>This animation happens on load (via the advanced class name <code>ani--load</code>). It works especially well for media like images, but also anything else.<br><br>(If you don't need this, you can disable it in <code>blocks/block-library/extended-ani/script.js</code>)<br></p>
<!-- /wp:paragraph --></div></div>
<!-- /wp:cover -->

<!-- wp:cover {"dimRatio":10,"overlayColor":"0-000","minHeight":50,"minHeightUnit":"vh","align":"full"} -->
<div class="wp-block-cover alignfull" style="min-height:50vh"><span aria-hidden="true" class="wp-block-cover__background has-0-000-background-color has-background-dim-10 has-background-dim"></span><div class="wp-block-cover__inner-container"><!-- wp:heading {"textAlign":"center","textColor":"0-000"} -->
<h2 class="has-text-align-center has-0-000-color has-text-color">Stepped:</h2>
<!-- /wp:heading -->

<!-- wp:paragraph {"align":"center","textColor":"0-000"} -->
<p class="has-text-align-center has-0-000-color has-text-color">Some parent blocks like columns delegate animations to their children and increment the start time for each.</p>
<!-- /wp:paragraph -->

<!-- wp:columns {"style":{"spacing":{"margin":{"top":"15px","bottom":"15px"}}},"textColor":"0-000","aquaminClassNameAni":[{"value":"ani\u002d\u002dopacity","title":"Opacity"},{"value":"ani\u002d\u002dscale","title":"Scale"},{"value":"ani\u002d\u002dblur","title":"Blur"},{"value":"ani\u002d\u002dleft","title":"Left"}]} -->
<div class="wp-block-columns has-0-000-color has-text-color ani--opacity ani--scale ani--blur ani--left ani" style="margin-top:15px;margin-bottom:15px"><!-- wp:column -->
<div class="wp-block-column"><!-- wp:paragraph -->
<p class="">One</p>
<!-- /wp:paragraph --></div>
<!-- /wp:column -->

<!-- wp:column -->
<div class="wp-block-column"><!-- wp:paragraph {"align":"center"} -->
<p class="has-text-align-center">Two</p>
<!-- /wp:paragraph --></div>
<!-- /wp:column -->

<!-- wp:column -->
<div class="wp-block-column"><!-- wp:paragraph {"align":"center"} -->
<p class="has-text-align-center">Three</p>
<!-- /wp:paragraph --></div>
<!-- /wp:column -->

<!-- wp:column -->
<div class="wp-block-column"><!-- wp:paragraph {"align":"center"} -->
<p class="has-text-align-center">Four</p>
<!-- /wp:paragraph --></div>
<!-- /wp:column -->

<!-- wp:column -->
<div class="wp-block-column"><!-- wp:paragraph {"align":"center"} -->
<p class="has-text-align-center">Five</p>
<!-- /wp:paragraph --></div>
<!-- /wp:column --></div>
<!-- /wp:columns -->

<!-- wp:paragraph {"align":"center","textColor":"0-000"} -->
<p class="has-text-align-center has-0-000-color has-text-color">Another example is the Buttons block which animates each Button block within it.</p>
<!-- /wp:paragraph -->

<!-- wp:buttons {"layout":{"type":"flex","justifyContent":"center"},"style":{"spacing":{"margin":{"top":"15px","bottom":"15px"}}},"aquaminClassNameAni":[{"value":"ani\u002d\u002dopacity","title":"Opacity"},{"value":"ani\u002d\u002dscale","title":"Scale"},{"value":"ani\u002d\u002dup","title":"Up"},{"value":"ani\u002d\u002d3d","title":"3D"}]} -->
<div class="wp-block-buttons ani--opacity ani--scale ani--up ani--3d ani" style="margin-top:15px;margin-bottom:15px"><!-- wp:button -->
<div class="wp-block-button"><a class="wp-block-button__link">Button 1</a></div>
<!-- /wp:button -->

<!-- wp:button -->
<div class="wp-block-button"><a class="wp-block-button__link">Button 2</a></div>
<!-- /wp:button -->

<!-- wp:button -->
<div class="wp-block-button"><a class="wp-block-button__link">Button 3</a></div>
<!-- /wp:button -->

<!-- wp:button -->
<div class="wp-block-button"><a class="wp-block-button__link">Button 4</a></div>
<!-- /wp:button -->

<!-- wp:button -->
<div class="wp-block-button"><a class="wp-block-button__link">Button 5</a></div>
<!-- /wp:button --></div>
<!-- /wp:buttons --></div></div>
<!-- /wp:cover -->

<!-- wp:cover {"dimRatio":10,"overlayColor":"0-000","minHeight":50,"minHeightUnit":"vh","align":"full"} -->
<div class="wp-block-cover alignfull" style="min-height:50vh"><span aria-hidden="true" class="wp-block-cover__background has-0-000-background-color has-background-dim-10 has-background-dim"></span><div class="wp-block-cover__inner-container"><!-- wp:heading {"textAlign":"center","textColor":"0-000"} -->
<h2 class="has-text-align-center has-0-000-color has-text-color">Opacity:</h2>
<!-- /wp:heading -->

<!-- wp:paragraph {"align":"center","placeholder":"Write title…","textColor":"0-000","aquaminClassNameAni":[{"value":"ani\u002d\u002dopacity","title":"Opacity"}]} -->
