<!-- /wp:paragraph -->

<!-- wp:paragraph {"align":"center","textColor":"0-000","className":"has-0-0-color has-text-color","aquaminClassNameAni":[]} -->
<p class="has-text-align-center has-0-0-color has-text-color has-0-000-color"><br>This animation happens on load (via the advanced class name <code>ani--load</code>). It works especially well for media like images, but also anything else.<br></p>
<!-- /wp:paragraph --></div></div>
<!-- /wp:cover -->

<!-- wp:cover {"dimRatio":10,"overlayColor":"0-000","minHeight":100,"minHeightUnit":"vh","isDark":false,"align":"full"} -->
