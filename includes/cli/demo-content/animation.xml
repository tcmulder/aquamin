  <content:encoded><![CDATA[<!-- wp:paragraph -->
<p class="">Here are common animations. Apply to your own blocks via <code>assets/block-edits/ani/index.js@affectedBlocks</code>.</p>
<!-- /wp:paragraph -->

<!-- wp:cover {"dimRatio":10,"overlayColor":"0-000","minHeight":100,"minHeightUnit":"vh","isDark":false,"align":"full"} -->
