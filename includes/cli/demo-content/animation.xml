<!-- /wp:heading -->

<!-- wp:paragraph {"align":"center","placeholder":"Write title…","backgroundColor":"0-000","textColor":"0-900","aquaminClassNameAni":[{"value":"ani\u002d\u002d3d","title":"3D"}]} -->
<p class="has-text-align-center has-0-900-color has-0-000-background-color has-text-color has-background ani--3d ani">3d</p>
<!-- /wp:paragraph --></div></div>
<!-- /wp:cover -->

<!-- wp:cover {"dimRatio":10,"overlayColor":"0-000","minHeight":50,"minHeightUnit":"vh","align":"full"} -->
<div class="wp-block-cover alignfull" style="min-height:50vh"><span aria-hidden="true" class="wp-block-cover__background has-0-000-background-color has-background-dim-10 has-background-dim"></span><div class="wp-block-cover__inner-container"><!-- wp:heading {"textAlign":"center","textColor":"0-000"} -->
<h2 class="has-text-align-center has-0-000-color has-text-color">Grow (scale-x):</h2>
<!-- /wp:heading -->

<!-- wp:separator {"backgroundColor":"0-000","className":"is-style-dots","aquaminClassNameAni":[{"value":"ani\u002d\u002dscale-x","title":"Grow"}]} -->
