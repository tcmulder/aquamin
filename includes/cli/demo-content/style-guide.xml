<!-- start color swatch -->
<script id="color-swatch">
[
	...[...document.styleSheets].find((sheet) => {
		return sheet?.href?.includes('theme.bundle.css');
	}).cssRules,
]
	.filter((rule) => {
		return rule.type === 1;
	})
	.reduce((propValArr, rule) => {
		const props = [...rule.style]
			.map((propName) => {
				return [
					propName.trim(),
					rule.style.getPropertyValue(propName).trim(),
				];
			})
			.filter(([propName]) => {
				return propName.indexOf('--') === 0;
			});

		return [
			...propValArr,
			...props
				.map((prop) => {
					return prop[0];
				})
				.filter((name) => {
					return /^--c-\d-\d$/.test(name);
				}),
		];
	}, [])
	.forEach((color) => {
		let swatch = document.querySelector('.color-swatch');
		if (!swatch) {
			const js = document.getElementById('color-swatch');
			swatch = document.createElement('div');
			swatch.classList.add('text-element', 'color-swatch');
			swatch.innerHTML = `
				<h2 class="text-level-semantics-title">Color Swatch</h2>
				<style>
					.color-swatch {
						display: flex;
						flex-wrap: wrap;
					}
					.color-swatch .text-level-semantics-title {
						width: 100%;
					}
					.color-swatch div {
						padding: .5em;
						color: white;
						width: 33.333%;
						text-shadow: 0 0 2px black;
					}
				</style>
			`;
			js.parentNode.insertBefore(swatch, js);
			const el = document.createElement('div');
			el.innerHTML = '--c-1-5 25% opacity';
			el.style.backgroundColor = `color-mix(in hsl, var(--c-1-5) 25%, transparent)`;
			swatch.appendChild(el);
		}
		const el = document.createElement('div');
		el.innerHTML = color;
		el.style.backgroundColor = `var(${color})`;
		swatch.appendChild(el);
	});
</script>

<!-- end color swatch -->

