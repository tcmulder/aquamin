{
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"layout": {
			"type": "constrained",
			"contentSize": "var(--w-m)",
			"wideSize": "var(--w-l)"
		},
		"styles": {
			"spacing": {
				"padding": {
					"right": "var(--gutter)",
					"left": "var(--gutter)"
				},
				"blockGap": null
			}
		},
		"spacing": {
			"blockGap": false,
			"units": ["%", "px", "em", "rem", "vh", "vw"],
			"spacingSizes": [
				{
					"size": "var(--sp-rem-s)",
					"slug": "10",
					"name": "Small"
				},
				{
					"size": "var(--gutter)",
					"slug": "20",
					"name": "Gutter"
				},
				{
					"size": "clamp(var(--sp-rem), 6vw, var(--sp-rem-m))",
					"slug": "50",
					"name": "Medium"
				},
				{
					"size": "clamp(var(--sp-rem-m), 15vw, var(--sp-rem-l))",
					"slug": "80",
					"name": "Large"
				}
			]
		},
		"typography": {
			"fontSizes": [
				{
					"slug": "h1",
					"size": "var(--fs-h1, 32px)",
					"name": "Like h1"
				},
				{
					"slug": "h2",
					"size": "var(--fs-h2, 24px)",
					"name": "Like h2"
				},
				{
					"slug": "h3",
					"size": "var(--fs-h3, 20.8px)",
					"name": "Like h3"
				},
				{
					"slug": "h4",
					"size": "var(--fs-h4, 16px)",
					"name": "Like h4"
				},
				{
					"slug": "h5",
					"size": "var(--fs-h5, 12.8px)",
					"name": "Like h5"
				},
				{
					"slug": "h6",
					"size": "var(--fs-h6, 11.2px)",
					"name": "Like h6"
				},
				{
					"slug": "big",
					"size": "var(--fs-big, 18px)",
					"name": "Big"
				},
				{
					"slug": "p",
					"size": "var(--fs-normal, 16px)",
					"name": "Normal (p)"
				},
				{
					"slug": "small",
					"size": "var(--fs-small, 14px)",
					"name": "Small"
				}
			]
		},
		"color": {
			"custom": false,
			"customGradient": false,
			"gradients": [],
			"link": false,
			"defaultPalette": false,
			"defaultGradients": false,
			"palette": [
				{
					"slug": "1-500",
					"color": "aquamarine",
					"name": "Primary"
				},
				{
					"slug": "2-500",
					"color": "tan",
					"name": "Secondary"
				},
				{
					"slug": "0-000",
					"color": "#000",
					"name": "Black"
				},
				{
					"slug": "0-900",
					"color": "#fff",
					"name": "White"
				}
			]
		}
	}
}
